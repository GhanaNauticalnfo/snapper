{
  "name": "@ghanawaters/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start:dev": "nest start --watch",
    "db:up": "docker compose -f docker/local/docker-compose.yml up -d",
    "db:down": "docker compose -f docker/local/docker-compose.yml down",
    "db:logs": "docker compose -f docker/local/docker-compose.yml logs -f",
    "db:restart": "npm run db:down && npm run db:up",
    "log:artemis": "docker compose -f docker/local/docker-compose.yml logs -f artemis",
    "build:cli": "tsc -p apps/api/tsconfig.cli.json",
    "typeorm:ts": "ts-node -r tsconfig-paths/register -P apps/api/tsconfig.app.json ./node_modules/typeorm/cli.js --dataSource apps/api/src/datasource.ts",
    "typeorm:js": "node ./node_modules/typeorm/cli.js --dataSource dist/apps/api/cli_build/datasource.js",
    "migration:create": "ts-node -r tsconfig-paths/register -P apps/api/tsconfig.app.json ./node_modules/typeorm/cli.js migration:create",
    "migration:generate": "npm run typeorm:ts -- migration:generate apps/api/src/migrations/%npm_config_name%",
    "migration:run:dev": "npm run typeorm:ts -- migration:run",
    "migration:revert:dev": "npm run typeorm:ts -- migration:revert",
    "migration:run": "npm run typeorm:js -- migration:run",
    "migration:revert": "npm run typeorm:js -- migration:revert",
    "migration:check": "npm run typeorm:js -- migration:show",
    "migration:check:dev": "npm run typeorm:ts -- migration:show",
    "db:reset:dev": "npm run typeorm:ts -- schema:drop && npm run migration:run:dev",
    "db:reset:test": "node scripts/reset-database.js",
    "typeorm": "npm run typeorm:js --",
    "dev:setup": "npm run db:up && npm run migration:run:dev"
  },
  "private": true,
  "devDependencies": {
